{
  "name": "aws-devkit",
  "description": "AWS DevKit for Node.js",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "npx nx build",
    "build:all": "npx nx run-many --target=build",
    "build:affeced": "npx nx affected --target=build",
    "test": "npx nx test",
    "test:all": "npx nx run-many --target=test",
    "test:affeced": "npx nx affected --target=test",
    "lint": "npx nx lint",
    "lint:all": "npx nx run-many --target=lint",
    "lint:affeced": "npx nx affeced --target=lint",
    "lint:staged": "npx lint-staged",
    "format:all": "npx nx format:write --all",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "private": false,
  "repository": {
    "url": "https://github.com/aws-devkit/aws-devkit.git"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.2.1",
    "@aws-lambda-powertools/tracer": "^1.2.1",
    "nanoid": "^4.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@commitlint/config-nx-scopes": "^17.0.0",
    "@commitlint/cz-commitlint": "^17.1.2",
    "@nrwl/cli": "14.8.2",
    "@nrwl/esbuild": "14.8.2",
    "@nrwl/eslint-plugin-nx": "14.8.2",
    "@nrwl/jest": "14.8.2",
    "@nrwl/js": "14.8.2",
    "@nrwl/linter": "14.8.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.8.2",
    "@theunderscorer/nx-semantic-release": "^1.14.0",
    "@types/jest": "^29.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "commitizen": "^4.2.5",
    "esbuild": "^0.15.7",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "ngx-deploy-npm": "^4.2.5",
    "nx": "14.8.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
